{
    "data": {
        "dataset_name": "mnist",
        "data_path": null,
        "num_clients": 5,
        "distribution": "non-iid",
        "shards_per_client": 2,
        "dirichlet_alpha": 0.5,
        "min_samples_per_client": 10,
        "max_samples_per_client": null,
        "validation_split": 0.1,
        "normalize": true,
        "augmentation": false
    },
    "model": {
        "architecture": "cnn",
        "input_shape": [
            28,
            28,
            1
        ],
        "num_classes": 10,
        "dropout_rate": 0.5,
        "use_batch_norm": false,
        "hidden_units": [
            128,
            64
        ]
    },
    "federated": {
        "num_rounds": 10,
        "clients_per_round": 4,
        "local_epochs": 2,
        "batch_size": 64,
        "client_learning_rate": 0.01,
        "server_learning_rate": 1.0,
        "client_optimizer": "sgd",
        "server_optimizer": "sgd",
        "use_tff": false,
        "aggregation_method": "fedavg",
        "fedprox_mu": 0.01
    },
    "privacy": {
        "enable_dp": false,
        "dp_noise_multiplier": 1.0,
        "dp_l2_norm_clip": 1.0,
        "dp_delta": 1e-05,
        "secure_aggregation": false
    },
    "training": {
        "seed": 42,
        "checkpoint_interval": 10,
        "early_stopping_patience": 20,
        "early_stopping_min_delta": 0.001,
        "verbose": 1
    },
    "output": {
        "results_dir": "results",
        "save_model": true,
        "save_checkpoints": true,
        "save_logs": true,
        "save_plots": true,
        "log_level": "INFO"
    }
}